{
  "packages": [
    "materials/*"
  ],
  "version": "2.6.0",
  "command": {
    "publish": {
      "conventionalCommits": true,
      "yes": true,
      "ignoreChanges": [
        "ignored-file",
        "*.md"
      ],
      "message": "chore(release): publish %s"
    },
    "version": {
      "allowBranch": "master"
    }
  }
}
